# Makefile generated by create_makefile.sh
FLAG := -R . VCT
default : all.vo

clean :
	rm -f .*.aux *.glob *.vo

Contracts.vo : Contracts.v Sets.vo
	coqc ${FLAG} Contracts.v

Sets.vo : Sets.v
	coqc ${FLAG} Sets.v

Logic_Contract.vo : Logic_Contract.v Sets.vo Contracts.vo
	coqc ${FLAG} Logic_Contract.v

Simple_Logic.vo : Simple_Logic.v Sets.vo Contracts.vo Logic_Contract.vo
	coqc ${FLAG} Simple_Logic.v

all.vo : all.v Contracts.vo Logic_Contract.vo Simple_Logic.vo
	coqc ${FLAG} all.v
